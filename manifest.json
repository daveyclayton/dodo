{
    "manifest_version": 3,
    "name": "BirdsOfPrey",
    "description": "Morphing Falcons into Eagles. Celtra CA v1 to v2 migration extension.",
    "version": "2.0.0",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "assets/icon128.png"
    },
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "permissions": [
        "notifications",
        "storage",
        "tabs"
    ],
    "options_page": "options/options.html",
    "host_permissions": [
        "https://hub.celtra.io/",
        "https://request-passthrough-afb95a0643d0.herokuapp.com/"
    ]
}
